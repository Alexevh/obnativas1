<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="16dp"
    android:paddingLeft="64dp"
    android:paddingRight="64dp"
    android:paddingTop="16dp"
    tools:context="com.example.obnativasp1.MainActivity"

    >

    <TextView
        android:layout_width="210dp"
        android:layout_height="66dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="41dp"
        android:layout_marginRight="96dp"
        android:layout_marginBottom="521dp"
        android:text="Registro!" />

    <EditText
        android:id="@+id/nombre"
        android:layout_width="287dp"
        android:layout_height="47dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="361dp"
        android:text="tu nombre papa!" />

    <EditText
        android:id="@+id/apellido"
        android:layout_width="294dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="51dp"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="273dp"
        android:text="tu apellido papa!" />

    <EditText
        android:id="@+id/password"
        android:layout_width="294dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="49dp"
        android:layout_marginRight="4dp"
        android:layout_marginBottom="203dp"
        android:text="tu pass papa!" />

    <EditText
        android:id="@+id/mail"
        android:layout_width="294dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="53dp"
        android:layout_marginBottom="473dp"
        android:text="tu mail papa!" />

    <EditText
        android:id="@+id/telefono"
        android:layout_width="299dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="48dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="136dp"
        android:text="tu telefono papa!" />
    <Button
        android:id="@+id/button1"
        android:layout_width="332dp"
        android:layout_height="73dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="75dp"
        android:layout_marginRight="68dp"
        android:layout_marginBottom="15dp"
        android:onClick="registrar"
        android:text="Registro" />


</RelativeLayout>