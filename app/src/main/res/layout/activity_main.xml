<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="16dp"
    android:paddingLeft="64dp"
    android:paddingRight="64dp"
    android:paddingTop="16dp"
    tools:context="com.example.obnativasp1.MainActivity"

    >

    <TextView
        android:id="@+id/texto"
        android:layout_width="299dp"
        android:layout_height="63dp"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="358dp"
        android:text="Registro!" />

    <EditText
        android:id="@+id/nombre"
        android:layout_width="600dp"
        android:layout_height="47dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="361dp"
        android:text="tu nombre papa!" />

    <EditText
        android:id="@+id/apellido"
        android:layout_width="600dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="273dp"
        android:text="tu apellido papa!" />

    <EditText
        android:id="@+id/password"
        android:layout_width="600dp"
        android:layout_height="49dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="196dp"
        android:text="tu pass papa!" />

    <EditText
        android:id="@+id/mail"
        android:layout_width="600dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="445dp"
        android:text="tu mail papa!" />

    <EditText
        android:id="@+id/telefono"
        android:layout_width="600dp"
        android:layout_height="46dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="119dp"
        android:text="tu telefono papa!" />

    <Button
        android:id="@+id/button1"
        android:layout_width="288dp"
        android:layout_height="103dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="21dp"
        android:layout_marginBottom="10dp"
        android:onClick="registrar"
        android:text="Registro" />


</RelativeLayout>